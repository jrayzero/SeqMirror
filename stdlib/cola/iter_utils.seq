def scan(a,b,*items):
  iters = [iter(b)] + [iter(item) for item in items]
  for i in a:
    yielded = [i]
    done = False
    for it in iters:            
      if it.done():
        done = True
        break
      else:
        yielded.append(it.next())
    if done:
      break
    yield yielded
  for it in iters:
    it.destroy()
